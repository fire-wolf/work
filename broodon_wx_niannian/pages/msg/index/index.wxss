
.paper{
  position: relative;
  width: 100%;
  height: 950rpx;
  margin: 72rpx auto 0;
}
.paper .slide-image-wrap{
  position: relative;
  width: 495rpx;
  border-radius: 12rpx;
  margin:0 auto; 
  -webkit-box-shadow: 0px 0px 20rpx 0px rgba(0,0,0,0.14);
  box-shadow: 0px 0px 20rpx 0px rgba(0,0,0,0.14);
}
.paper .slide-image{
  display: block;
  width: 495rpx;
  margin: 0 auto;
  border-radius: 12rpx;
}
.paper .music-icon{
  position: absolute;
  top: 20rpx;
  right: 20rpx;
  background: rgba(255, 255, 255, 0.6);
  border-radius: 68rpx;
  padding: 14rpx;
  z-index: 10;
}
.paper .music-icon .icon{
  display: block;
  width: 40rpx;
  margin: 0 auto 38rpx 0;
}
.paper .music-icon .icon-play,.paper .music-icon .icon-close{
  display: block;
  width: 38rpx;
  height: 38rpx;
}
.rotate{
  -webkit-animation: playRotate 3s linear infinite;
  animation: playRotate 3s linear infinite;
}

@-webkit-keyframes playRotate{
  0% {transform:rotateZ(0deg);}
  25% {transform:rotateZ(90deg);}
  50% {transform:rotateZ(180deg);}
  75% {transform:rotateZ(270deg);}
  100% {transform:rotateZ(360deg);}
}
@keyframes playRotate{
  0% {transform:rotateZ(0deg);}
  25% {transform:rotateZ(90deg);}
  50% {transform:rotateZ(180deg);}
  75% {transform:rotateZ(270deg);}
  100% {transform:rotateZ(360deg);}
}


.btn{
  width:446rpx;
  height:94rpx; 
  line-height: 94rpx;
  font-size: 36rpx;
  color: #fff;
  text-align: center;
  background:#69BF30;
  border-radius: 47rpx;
  margin: 20rpx auto; 
}