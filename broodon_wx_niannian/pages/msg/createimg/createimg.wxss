/* 分享到朋友圈 */
 .share-pyq{
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  z-index: 5;
  background:#fff; 
  
}
 .share-pyq .pyq-main{
  width: 590rpx;
  height: 970rpx;
  background: #fff;
  margin: 105rpx auto 0 auto;
  border-radius: 8rpx; 
}

 
 .pyq-main .canvas{
  position: relative;
  width: 590rpx;
  height: 723rpx;
}
 .pyq-main .canvas .bg-share-pyq{
  display: block;
  width: 590rpx;
  height: 723rpx;
}
 .pyq-main .canvas .share-username{
  position: absolute;
  top: 32rpx;
  width: 100%;
  height:48rpx; 
  font-size:34rpx;
  color:#3B3635;
  line-height:48rpx;
  text-align: center;
}
 .pyq-main .canvas .qr-code{
  display: block;
  position: absolute;
  top: 172rpx;
  left: 203rpx;
  width: 186rpx;
  height:186rpx; 
}

 .pyq-main .have-save{
  font-size:24rpx;
  color:#9E938E;
  line-height:33rpx;
  text-align: center;
  padding-top: 33rpx;
}
 
 .pyq-main .save-btn{
  width:517rpx;
  height:94rpx; 
  border-radius: 47rpx ; 
  font-size:36rpx;
  color:#fff;
  line-height:94rpx;
  text-align: center;
  background: #69BF30;
  margin: 107rpx auto 0 auto;
}

#myCanvas{
  position: fixed;
  top: 2000rpx;
  width: 1100px;
  height: 1350px;
}