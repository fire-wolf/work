.contain{
  position: fixed; 
  width: 100%;
  height: 100%;
  background: #F7F7F7;
  z-index: 1;
}
.redpacket-box{
  position: relative;
  margin-top: 16rpx;
  padding:0 32rpx;
  width: 100%;
  height: 112rpx;
  line-height: 112rpx;
  background: #fff;
}
.redpacket-box .title{
  float: left;
  width: 128rpx;
  height: 112rpx;
  line-height: 112rpx;
  font-size:32rpx;
  color:#595350;
}
.redpacket-box input{
  float: right;
  max-width:300rpx;
  height: 112rpx;
  line-height: 112rpx;
  text-align: right;
}
.redpacket-btn{ 
  width:100%;
  height:100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size:32rpx;
  color:#fff !important;
  background:linear-gradient(90deg,rgba(122,221,94,1),rgba(79,202,124,1));
  border:none!important; 
}
.redpacket-btn[disabled]{
  background: #B3B9B5 !important; 
}