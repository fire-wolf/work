page {
  background-color: #f7f7f7;
}
.identification {
  width: 100%;
  height: 88rpx;
  background: rgba(255, 235, 163, 1);
  padding-left: 34rpx;
  display: flex;
  flex-direction: row;
}

.identification image {
  width: 40rpx;
  height: 40rpx;
  margin:24rpx 17rpx 0 0;
}

.identification .tip {
  font-size: 28rpx;
  color: rgba(230, 174, 80, 1);
  letter-spacing: 1rpx;
  line-height: 88rpx;
}
.identification button{
  margin:0;
  font-size: 28rpx;
  color: rgba(230, 174, 80, 1);
  line-height: 88rpx;
  text-decoration: underline;
  border: none;
  padding: 0;
  letter-spacing: 1rpx;
  margin-right: 2rpx;
}
.head {
  width: 100%;
  height: 356rpx;
  background:linear-gradient(90deg,rgba(122,221,94,1),rgba(79,202,124,1));
  padding-top: 80rpx;
}

.head .balance {
  width: 100%;
  text-align: center;
  height: 40rpx;
  font-size: 28rpx;
  color: rgba(255, 255, 255, 1);
  line-height: 40rpx;
}

.head .money {
  width: 100%;
  text-align: center;
  height: 93rpx;
  font-size: 80rpx;
  font-weight: bold;
  color: rgba(255, 255, 255, 1);
  line-height: 93rpx;
  margin-top: 12rpx;
}

.head .put-money {
  margin: 25rpx auto 0;
  width: 132rpx;
  height: 60rpx;
  border-radius: 6rpx;
  border: 3rpx solid;
  font-size: 24rpx;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  line-height: 60rpx;
}

.item {
  width: 100%;
  height: 112rpx;
  padding-left: 32rpx;
  position: relative;
  background-color: #fff;
}

.item .inner {
  border-bottom: solid 1rpx rgba(227, 234, 230, 1);
  height: 112rpx;
  padding-top: 34rpx;
}

.item .inner .date-img {
  width: 40rpx;
  height: 40rpx;
  margin-right: 21rpx;
  margin-top: 6rpx;
  float: left;
}

.item  .inner text {
  width: 128rpx;
  height: 45rpx;
  font-size: 32rpx;
  color: rgba(89, 83, 80, 1);
  line-height: 45rpx;
}

.item .inner .right {
  float: right;
  width: 14rpx;
  height: 24rpx;
  margin: 10rpx 41rpx 0 0;
}
