.prefeeling-cont{
  width: 95.4%;
  margin: 20rpx auto 0 auto;
  border-radius: 12rpx;
}
.prefeeling-cont .red-letter{
  display: block;
  width: 100%;
  
  border-radius: 8rpx 8rpx 0 0;
}
.prefeeling-cont .cont-main{
  min-height: 700rpx;
  padding: 0 31rpx 15rpx 31rpx; 
  padding-bottom: 15rpx;
  margin: 0 auto;
  /* overflow-y:scroll;  */
}
.prefeeling-cont .cont-main .feeling-salutation{
  color: #595350;
  font-size: 32rpx;
  font-weight: bold;
  line-height:45rpx;
  height:45rpx;
}
.prefeeling-cont .cont-main .feeling-text{
  color: #3B3635;
  font-size: 28rpx;
  line-height:50rpx;
  min-height: 480rpx;
  padding-top:17rpx;
}
.prefeeling-cont .cont-main .feeling-signature{
  text-align: right;
  height:45rpx; 
  font-size:32rpx;
  color:#595350;
  line-height:45rpx;
  margin: 50rpx 0 4rpx 0;
}

.prefeeling-cont .complete-btn{
  width:80%;
  height:94rpx; 
  line-height: 94rpx;
  color: #fff;
  font-size: 36rpx;
  background:#69BF30;
  margin: 40rpx auto 12rpx auto;
  border-radius: 10rpx; 
}
.prefeeling-cont .tip-text{
  height:56rpx; 
  line-height: 30rpx;
  font-size:22rpx;
  color: #BAB2AF;
  text-align: center;
  padding: 13rpx 0;
}